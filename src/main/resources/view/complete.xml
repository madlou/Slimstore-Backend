<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:context="http://www.springframework.org/schema/context"
  xmlns:mongo="http://www.springframework.org/schema/data/mongo"
  xsi:schemaLocation="http://www.springframework.org/schema/data/mongo http://www.springframework.org/schema/data/mongo/spring-mongo-1.0.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.2.xsd
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">
    
    <bean class="com.tjx.lew00305.slimstore.model.common.View">
        <property name="name" value="complete" />
        <property name="title" value="Transaction Complete" />
        <property name="message" value="The transaction has completed, press F1 to continue a new transaction." />
        <property name="formSuccess" value="" />
        <property name="formElements">
            <array>
            </array>
        </property>
        <property name="functionButtons">
            <array>
                <bean class="com.tjx.lew00305.slimstore.model.common.FunctionButton">
                    <property name="position" value="1" />
                    <property name="label" value="Complete" />
                    <property name="action" value="home" />
                    <property name="process" value="TransactionComplete" />
                </bean>
            </array>
        </property>
    </bean>

</beans>
